#include<stdio.h>
#include<math.h>
#include<stdlib.h>

int main()
{
  int t,n,d,i,k=0,k2=0,x;
   scanf("%d",&t);
    while(t--){
    scanf("%d%d",&n,&d);

    for(i=0;i<=n;i++)
    {
      if(d%(i+1)==0)
      x=d/(i+1);
      else
      x=d/(i+1)+1;
      if((i+x)<=n){
      k=1;
      break;
      }
      if ((i+x)>n){
      k2=1;
      }
    }
    if(k==1)
    printf("Yes\n");
    if(k!=1&&k2==1)
    printf("No\n");
    k=0;k2=0;
  }

  return 0;
}
